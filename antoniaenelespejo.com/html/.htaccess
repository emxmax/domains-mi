# Activar RewriteEngine
RewriteEngine on

RewriteEngine On
RewriteCond %{HTTP_HOST} ^www.antoniaenelespejo.com [NC]
RewriteRule ^(.*)$ http://antoniaenelespejo.com/$1 [L,R=301]

RewriteRule ^tu-mejor-version/?$ tu-mejor-version.php
RewriteRule ^mujeres-que-inspiran/?$ mujeres-que-inspiran.php
RewriteRule ^nuestra-casa/?$ nuestra-casa.php
RewriteRule ^antonia/?$ antonia.php
RewriteRule ^videos/?$ detalle-videos.php

Rewriterule ^noticias/(.+)/?$ detalle.php?not_url=$1
# BEGIN ANSIBLE MANAGED BLOCK
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteCond %{HTTP_HOST} antoniaenelespejo.com.mediaimpactperu.com$
RewriteRule ^(.*)$ "http://mediaimpactperu.com/404" [R=301,L]
</IfModule>
# END ANSIBLE MANAGED BLOCK
AddHandler application/x-httpd-alt-php73___lsphp .php
DirectoryIndex index.html INDEX.HTML Index.html index.shtml index.cgi index.pl index.php index.xhtml index.htm
