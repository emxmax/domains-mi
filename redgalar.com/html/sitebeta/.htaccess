# Activar RewriteEngine
RewriteEngine on

RewriteCond %{HTTPS} off
RewriteRule (.*) https://www.redgalar.com/sitebeta/$1 [R=301,L,QSA]

Rewriterule ^productos/(.+)/?$ producto.php?pro_url=$1

Rewriterule ^categorias/(.+)/?$ categoria.php?cate_url=$1

Rewriterule ^socios/(.+)/?$ socio.php?so_url=$1

RewriteRule ^ayuda/?$ ayuda.php
RewriteRule ^registro/?$ registro.php
RewriteRule ^perfil/?$ perfil.php
RewriteRule ^carrito/?$ carrito.php
RewriteRule ^libro-de-reclamaciones/?$ libro-de-reclamaciones.php
RewriteRule ^gracias/?$ gracias.php