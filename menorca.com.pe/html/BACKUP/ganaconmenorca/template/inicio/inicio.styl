.logoContenedor
	align-items flex-end
	background-color verde_oscuro
	display flex
	flex-flow row no-wrap
	justify-content center
	.logoItem
		img
			position relative
			top 15px
			@media screen and (min-width:768px)
				top 25px
.proyectosContenedor
	align-items center
	border-bottom 10px solid verde
	display flex
	flex-flow row no-wrap
	justify-content center
	padding-top 60px
	padding-bottom 10px
	.proyecto
		align-self stretch 
		&.separador
			background-color verde_oscuro
			margin-left 30px
			margin-right 30px
			width 2px
		.alinearEstancia
			padding-top 8px
			@media screen and (min-width:768px)
				padding-top 28px
		img
			width 160px
			@media screen and (min-width:768px)
				width 350px
.procesoContenedor, .premioContenedor
	border-bottom 10px solid verde_claro
	display flex
	flex-flow column no-wrap
	justify-content flex-start
	padding-bottom 10px
	.proceso
		padding-bottom 21px
		&.hgroup
			padding-top 40px
			padding-bottom 40px
			h1
				text-align center
		.textoProceso
			display flex
			flex-flow row no-wrap
			justify-content flex-start
			padding-left 20px
			&:nth-child(n)
				margin-bottom 30px
			.contenidoTexto
				border 0px solid #f00
				padding-left 12px
				&:first-child
					padding-left 10px
					padding-right 10px
				h2
					margin-top 0px
				p
					padding-top 20px
					padding-bottom 20px
				a
					font-size 14px					
				span
					background-color verde_oscuro
					border-radius 31px
					color blanco
					display block
					height 31px
					text-align center
					padding 8px 0
					width 30px
				ul
					li
						color verde_oscuro
						p
							padding 0
				.contenedorBoton
					display flex
					flex-flow row no-wrap
					justify-content center
					button
						background transparent
						border 2px groove verde_claro
						border-radius 0px
						width 30%
.premioContenedor
	border-bottom 0px solid verde_oscuro
	padding-top 34px
	.textoProceso
		background plomo_claro
		padding-top 20px
		position relative
		.icon-vineta
			position absolute
			bottom -4px
			right 0px	