section.propietario(ng-controller="propietarioCrtl")
	include mainmenu.jade
	.container.inner-propietario
		.table-responsive
			table.table.table-striped.table-bordered
				thead
					tr
						th.center
							| #
						th.center
							p
								| Nombre del propietario
						th.center
							p
								| MÃ³vil
						th.center
							p
								| D.N.I
						th.center
							p
								| Descargar Excel
						th.center
							p
								| Fecha registro
						th.center
							i.glyphicon.glyphicon-pencil
				tbody
					tr(ng-repeat="data in propietario")
						td.center
							a(href="javascript:void(0)",ng-click="viewDetail(data.id)")
								| {{data.item}}
						td
							a(href="javascript:void(0)",ng-click="viewDetail(data.id)")
								| {{data.name}}
						td.center
							p
								| {{data.phone}}
						td.center
							p
								| {{data.dni}}
						td.center
							a(href="javascript:void(0)",ng-click="excelMyReferido(data.id)")
								| Excel
						td.center
							p
								| {{data.datecreate}}
						td.center
							a(href="javascript:void(0)", ng-click="viewDetail(data.id)")
								i.glyphicon.glyphicon-eye-open